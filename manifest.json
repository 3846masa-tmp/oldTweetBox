{
  "manifest_version": 2,
  "name": "Twitterのツイートボックスを元に戻す拡張機能",
  "version": "0.3846.1",

  "description": "つまり，そういうことです．",

  "content_scripts": [
    {
      "matches": ["https://twitter.com/*", "http://twitter.com/*"],
      "js": ["jquery-2.1.1.min.js", "oldTweetBox.js"],
      "run_at": "document_idle"
    }
  ]
}
